    <constant name="struts.devMode" value="false" />   
    <constant name="struts.custom.i18n.resources" value="global"></constant>
    <constant name="struts.action.excludePattern" value="/NewWebService,/SfUserWebService"/>
    <!--<constant name="struts.locale" value="en_US" />-->
    <!--<constant name="struts.convention.action.packages" value="org.aepscolombia.platform.controllers"/>-->     
    <constant name="struts.i18n.reload" value="false"/>
    <constant name="struts.configuration.xml.reload" value="false"/>
    <package name="principal" extends="struts-default,json-default" namespace="/">
        <interceptors>
            <interceptor name="requireUser" class="org.aepscolombia.platform.interceptors.RequireUserInterceptor"/>
            <interceptor name="validateI18n" class="org.aepscolombia.platform.interceptors.ChangeI18n"/>
            <interceptor name="viewAction" class="org.aepscolombia.platform.interceptors.ViewActionInterceptor"/>
            <interceptor-stack name="defaultStackAeps">    
                <interceptor-ref name="requireUser" />
                <interceptor-ref name="defaultStack" />        
            </interceptor-stack>
            <interceptor-stack name="defaultI18n">    
                <interceptor-ref name="timer" />
                <interceptor-ref name="defaultStack" />
                <interceptor-ref name="logger" />
                <interceptor-ref name="validateI18n" />
            </interceptor-stack>
        </interceptors>
        
        <global-results>
            <result name="400">
                /WEB-INF/views/errors/400.jsp                
            </result>
            <result name="401">
                /WEB-INF/views/errors/401.jsp                
            </result>
            <result name="403">
                /WEB-INF/views/errors/403.jsp
            </result>
            <result name="404">
                /WEB-INF/views/errors/404.jsp
            </result>
        </global-results>
        <!-- Public part -->
        <action name="*" class="org.aepscolombia.platform.controllers.BaseAction">
            <result name="input">/WEB-INF/views/errors/404.jsp</result>
        </action>
        <action name="locale" class="org.aepscolombia.platform.controllers.LocaleAction">
            <!--<result name="states" type="json"></result>-->
            <result>/layout.jsp</result>
        </action>
        
        <action name="localeLogin" class="org.aepscolombia.platform.controllers.LocaleAction">
            <result>/WEB-INF/views/login/login.jsp</result>            
        </action>
        
        <action name="localePrivate" class="org.aepscolombia.platform.controllers.LocaleAction">
            <result>/WEB-INF/views/generals/layoutPrivate.jsp</result>
        </action>
        
        <action name="initial">
            <interceptor-ref name="validateI18n"/>
            <interceptor-ref name="viewAction"/>
            <result type="redirectAction">locale</result>
            <!--<result>/layout.jsp</result>-->
        </action>             
        <action name="home">
            <interceptor-ref name="validateI18n"/>
            <interceptor-ref name="viewAction"/>
            <result name="page">/layout.jsp</result>
            <result>/WEB-INF/views/generals/home.jsp</result>
        </action>     
        <action name="about">
            <interceptor-ref name="validateI18n"/>
            <interceptor-ref name="viewAction"/>
            <result name="page">/layout.jsp</result>
            <result>/WEB-INF/views/generals/aboutUs.jsp</result>
        </action>           
        <action name="contact" method="getReport" class="org.aepscolombia.platform.controllers.ActionContact">
        <!--<action name="contact">-->
            <interceptor-ref name="validateI18n"/>
            <interceptor-ref name="viewAction"/>
            <result name="page">/layout.jsp</result>
            <result>/WEB-INF/views/generals/contacts.jsp</result>
        </action>
        <action name="verifyUser"  method="verifyUser" class="org.aepscolombia.platform.controllers.ActionLogin">
<!--            <result>/WEB-INF/views/layoutPrivate.jsp</result>-->
            <interceptor-ref name="basicStack"/>
            <result type="redirectAction">accessPlatform</result>
<!--            <result name="error" type="redirectAction">400</result>
            <result name="input" type="redirectAction">404</result>-->
        </action> 
        <action name="accessPlatform">
            <result>/WEB-INF/views/generals/accessPlatform.jsp</result>
        </action>
        <action name="restorePassword"  method="restorePassword" class="org.aepscolombia.platform.controllers.ActionLogin">
            <interceptor-ref name="jsonValidationWorkflowStack"/>
            <result name="states" type="json"></result>
        </action>
        
        <action name="sendInformation"  method="sendInformation" class="org.aepscolombia.platform.controllers.ActionLogin">
            <interceptor-ref name="jsonValidationWorkflowStack"/>
            <result name="states" type="json"></result>
        </action>
        
        
        <action name="verifyUserToRestore"  method="verifyUserToRestore" class="org.aepscolombia.platform.controllers.ActionLogin">
            <interceptor-ref name="validateI18n"/>
            <interceptor-ref name="basicStack"/>
            <result>/WEB-INF/views/login/restorePass.jsp</result>
        </action> 
        <action name="changePassUser"  method="changePassUser" class="org.aepscolombia.platform.controllers.ActionLogin">
            <interceptor-ref name="jsonValidationWorkflowStack"/>
            <result name="states" type="json"></result>
        </action>
        <action name="login" method="login" class="org.aepscolombia.platform.controllers.ActionLogin">
            <interceptor-ref name="validateI18n"/>    
            <interceptor-ref name="jsonValidationWorkflowStack"/>        
            <interceptor-ref name="viewAction"/>                
            <result name="states" type="json"></result>   
            <result type="redirectAction">dashboard</result>            
            <result name="input">/WEB-INF/views/login/login.jsp</result>
        </action>
        <action name="saveUser" method="saveData" class="org.aepscolombia.platform.controllers.ActionLogin">    
            <interceptor-ref name="jsonValidationWorkflowStack"/>
            <interceptor-ref name="validateI18n"/> 
            <interceptor-ref name="defaultStack" />               
            <result name="states" type="json"></result>
        </action>
        <action name="viewIssue" method="show" class="org.aepscolombia.platform.controllers.ActionIssues">
            <interceptor-ref name="defaultStackAeps"/>
            <interceptor-ref name="validateI18n"/>            
            <interceptor-ref name="viewAction"/>            
            <result name="page">/WEB-INF/views/generals/layoutPrivate.jsp</result>
            <result>/WEB-INF/views/generals/error.jsp</result>
        </action>
        <action name="sendIssue" method="saveData" class="org.aepscolombia.platform.controllers.ActionIssues">
            <interceptor-ref name="jsonValidationWorkflowStack"/>
            <interceptor-ref name="defaultStackAeps"/>
            <result name="states" type="json"></result>
        </action>
        
        <action name="chargeValues" method="chargeValues" class="org.aepscolombia.platform.controllers.ActionContact">
            <interceptor-ref name="jsonValidationWorkflowStack"/>
            <result name = "states" type="json"></result>
        </action>
        
        <action name="sendMessage" method="sendMessage" class="org.aepscolombia.platform.controllers.ActionContact">
            <interceptor-ref name="jsonValidationWorkflowStack"/>
            <result name = "states" type="json"></result>
        </action>
        
        <!-- End public part -->
        
        <!-- Private part -->
        <action name="signin" method="signin" class="org.aepscolombia.platform.controllers.ActionLogin">
            <interceptor-ref name="requireUser"/>    
            <interceptor-ref name="validateI18n"/>    
            <interceptor-ref name="jsonValidationWorkflowStack"/>        
            <result type="redirectAction">dashboard</result>    
            <result name="states" type="json"></result>        
            <result name="input">/WEB-INF/views/login/login.jsp</result>
        </action>  
        <action name="tryFree" method="tryFree" class="org.aepscolombia.platform.controllers.ActionLogin">
            <interceptor-ref name="validateI18n"/>    
            <interceptor-ref name="jsonValidationWorkflowStack"/>        
            <result type="redirectAction">dashboard</result>    
            <result name="states" type="json"></result>        
            <result name="input">/WEB-INF/views/generals/home.jsp</result>
        </action>                         
        <action name="logout" method="logout" class="org.aepscolombia.platform.controllers.ActionLogin">
            <result type="redirectAction">
                <param name="actionName">initial</param>
                <param name="namespace">/</param>
            </result>
        </action>    
        <action name="dashboard">
            <interceptor-ref name="validateI18n"/>
            <interceptor-ref name="defaultStackAeps"/>                        
            <interceptor-ref name="viewAction"/>            
            <result name="page">/WEB-INF/views/generals/layoutPrivate.jsp</result>
        </action>
        <action name="homePrivate">
            <interceptor-ref name="defaultStackAeps"/>  
            <interceptor-ref name="validateI18n"/>                                  
            <interceptor-ref name="viewAction"/>            
            <result name="page">/WEB-INF/views/generals/layoutPrivate.jsp</result>
            <result>/WEB-INF/views/generals/dashboard.jsp</result>
        </action>
        <action name="getting" method="getValues" class="org.aepscolombia.platform.controllers.ActionProfile">
