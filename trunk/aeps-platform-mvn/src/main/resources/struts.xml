            <interceptor-ref name="jsonValidationWorkflowStack"/>
            <interceptor-ref name="defaultStackAeps"/>
            <result name = "states" type="json"></result>
        </action>
        <action name="saveSetting" method="saveSetting" class="org.aepscolombia.platform.controllers.ActionProfile">
            <interceptor-ref name="jsonValidationWorkflowStack"/>
            <interceptor-ref name="defaultStackAeps"/>
            <result name = "states" type="json"></result>
        </action>      
        <!-- End private part -->
    </package>  
    
