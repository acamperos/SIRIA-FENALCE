            <interceptor-ref name="validateI18n"/>            
            <interceptor-ref name="viewAction"/>            
            <result name="page">/WEB-INF/views/generals/layoutPrivate.jsp</result>
            <result>/WEB-INF/views/profile/form-profile.jsp</result>
        </action>          
        <!-- End private part -->
    </package>  
    
